{
  "name": "react-imagemagick",
  "version": "0.0.1",
  "description": "React component and abstractions to transform images using ImageMagick 100% in the browser",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine.js"
  },
  "keywords": [
    "imagemagick",
    "browser",
    "typescript",
    "images",
    "image processing",
    "reacts"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "imagemagick-browser": "file:../imagemagick-browser",
    "react": "^16.5.2",
    "wasm-imagemagick": "^1.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jasmine": "^3.3.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
