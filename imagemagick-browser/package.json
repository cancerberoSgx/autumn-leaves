{
  "name": "imagemagick-browser",
  "version": "0.0.1",
  "description": "Abstractions on top of WASM-ImageMagick to easy usage",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "npx concurrently --kill-others \"cd test-project1 && npm start\" \"sleep 10s && npm run test-jest\"",
    "test-jest": "export NODE_PATH=$(npm root --quiet -g) && npx jest"
  },
  "keywords": [
    "imagemagick",
    "browser",
    "typescript",
    "images",
    "image processing"
  ],
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "p-map": "^1.0.0",
    "wasm-imagemagick": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/p-map": "^1.0.0",
    "@types/puppeteer": "^1.6.4",
    "@types/sinon": "^5.0.1",
    "concurrently": "^4.0.1",
    "jest": "^23.3.0",
    "sinon": "^6.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
